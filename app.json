{
  "expo": {
    "name": "Word Imposter",
    "slug": "word-imposter",
    "version": "0.1.12",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wordimposter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raadsel.wordimposter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#5170ff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.raadsel.wordimposter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#5170ff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0fdd658a-b20c-426a-8ceb-51a291d39a3c"
      }
    }
  }
}
